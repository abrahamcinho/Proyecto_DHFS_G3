<%- include("./partials/header") %>
<h2>Usuarios</h2>
<a href="/usuarios/create/"><i class="fa-solid fa-user-plus"></i></a>
<ul>
    <% for (let i = 0; i < users.length; i++) { %>
        <li>
          <a href="/usuarios/detail/<%=users[i].user_id %>">
            <%= users[i].first_name + ' ' + users[i].last_name %>
          </a>
          <a href="/usuarios/edit/<%=users[i].user_id %>"><i class="fa-solid fa-pen-to-square"></i></a>
          <a href="/usuarios/delete/<%=users[i].user_id %>"><i class="fa-solid fa-trash-can"></i></a>
        </li>
      <% } %>
</ul>
<%- include("./partials/footer") %>